<AnchorPane id="AnchorPane" prefHeight="475.0" prefWidth="614.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ampm_gym_and_fitness.ResetPassController">
   <children>
      <Label fx:id="setL" alignment="CENTER" contentDisplay="CENTER" layoutX="145.0" layoutY="14.0" prefHeight="35.0" prefWidth="324.0" text="Reset Password" textFill="#4750c9">
         <font>
            <Font name="Bodoni MT Black Italic" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="pass" alignment="CENTER" layoutX="204.0" layoutY="158.0" prefHeight="25.0" prefWidth="207.0" promptText="Enter new password" />
      <TextField fx:id="retypePass" alignment="CENTER" layoutX="204.0" layoutY="223.0" prefHeight="25.0" prefWidth="207.0" promptText="Re - enter new password" />
      <Button layoutX="262.0" layoutY="307.0" mnemonicParsing="false" onAction="#resetPassButtonOnClick" prefHeight="35.0" prefWidth="91.0" text="Reset" />
      <Button layoutX="14.0" layoutY="436.0" mnemonicParsing="false" onAction="#backButtonOnClick" prefHeight="25.0" prefWidth="67.0" text="Back" />
   </children>
